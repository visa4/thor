{
  "name": "Dsign",
  "description": "Dsign",
  "devDependencies": {
    "bower": "^1.8.4",
    "devtron": "^1.4.0",
    "electron": "2.0.0",
    "electron-builder": "~20.11.1",
    "polymer-cli": "^1.5.4",
    "chai": "*",
    "mocha": "*"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron --inspect=5858 ./app --enable-logging",
    "dev": "NODE_ENV='development' npm run start",
    "dist": "build -l --x64 --ia32"
  },
  "build": {
    "appId": "com.app.dsign",
    "asar": false,
    "linux": {
      "target": "deb",
      "icon": "build"
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.9",
    "fluent-ffmpeg": "^2.1.2",
    "image-size": "^0.6.2"
  }
}
